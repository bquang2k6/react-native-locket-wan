{
  "expo": {
    "name": "locket-wan",
    "slug": "locket-wan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.wan.locket-wan",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Ứng dụng cần camera để chụp ảnh và quay video",
        "NSMicrophoneUsageDescription": "Ứng dụng không ghi âm, chỉ dùng để quay video"
      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "package": "com.quang2k6.locketwan",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      "expo-secure-store",
      "react-native-compressor"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "BASE_API_URL": "https://apilocketwan.traidep.site",
      "BASE_API_URL_DB": "https://api4locketwan.antiviet.click",
      "BASE_API_URL_WS": "https://wslocketwan.wibu.life",
      "router": {},
      "eas": {
        "projectId": "1d35bd34-381a-4ae8-8185-efb493676bc7"
      }
    },
    "owner": "wantech",
    "updates": {
      "url": "https://u.expo.dev/1d35bd34-381a-4ae8-8185-efb493676bc7",
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "enabled": true,
      "channel": "production"
    }
  }
}